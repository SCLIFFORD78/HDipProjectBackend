<div class="uk-margin uk-card uk-card-body uk-height-large uk-box-shadow-large" id="map"></div>
<script>
    var map = L.map('map').setView([52.05102, -9.6871], 11);

    L.tileLayer('https://api.maptiler.com/maps/basic/{z}/{x}/{y}.png?key=m3Lj5QIcWzzqBkEBCk7v', {
        attribution: '<a href="https://www.maptiler.com/copyright/" target="_blank">&copy; MapTiler</a> <a href="https://www.openstreetmap.org/copyright" target="_blank">&copy; OpenStreetMap contributors</a>',
    }).addTo(map);

</script>
{{#each hives}}
<script>
    var marker = L.marker([{{latitude}},{{longtitude}}]).addTo(map);
</script>

{{/each}}